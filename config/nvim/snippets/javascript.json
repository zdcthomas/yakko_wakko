{
  "if": {
    "prefix": "if",
    "body": [
      "if ($1) {",
      "  $2",
      "}"
    ],
    "description": "if statement"
  },
  "elif": {
    "prefix": "else if",
    "body": [
      "} else if ($1) {",
      "  $2"
    ],
    "description": "if statement"
  },
   "require": {
    "prefix": "req",
    "body": "require('${1:module}')"
  },
  "module.exports": {
    "prefix": "mod",
    "body": "module.exports = ${1:name}"
  },
  "expect": {
    "prefix": "expect",
    "body": "expect(${1:case})"
  },

  "strictEqual": {
    "prefix": ".toS",
    "body": ".toStrictEqual(${1:expected})"
  },
  "else": {
    "prefix": "else",
    "body": [
      "} else {"
    ],
    "description": "else branch"
  }
}
