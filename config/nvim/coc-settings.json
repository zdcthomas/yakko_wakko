{
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "codeLens.enable": true, 
  "diagnostic.virtualText": true,
  "suggest.snippetIndicator": "â–º",
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.maxWindowWidth": 100,
  "diagnostic.checkCurrentLine": true,
  "suggest.enablePreview": true,
  "rust-analyzer.inlayHints.chainingHints": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.cargo.allFeatures": true,
  "coc.preferences.formatOnSaveFiletypes": ["elixir", "rust", "elm", "typescript", "javascript", "typescriptreact", "typescript.tsx", "javascriptreact", "go"]
}
